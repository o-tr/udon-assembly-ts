{
  "name": "@ootr/udon-assembly-ts",
  "version": "0.0.4",
  "private": false,
  "license": "MIT",
  "description": "TypeScript to Udon Assembly transpiler and UdonSharp stubs",
  "type": "module",
  "bin": {
    "udon-assembly-ts": "./dist/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o-tr/udon-assembly-ts.git"
  },
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./stubs": {
      "types": "./src/stubs/index.ts",
      "import": "./dist/stubs/index.js"
    },
    "./stubs/*": {
      "types": "./src/stubs/*.ts",
      "import": "./dist/stubs/*.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --hideSkippedTests",
    "format": "biome format",
    "format:fix": "biome format --write",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "@biomejs/biome": "^2.3.14",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.15.1"
}
